RewriteEngine On
RewriteCond ${REQUEST_URI} ^.+$
RewriteRule ^birlik1(/?)$ birlik1.php
RewriteRule ^birlik(/?)$ birlik.php
RewriteRule ^basvurular(/?)$ basvurular.php
RewriteRule ^basvuru-incele/([^/\.]+)/?$ basvuru-incele.php?app_dbid=$1
RewriteRule ^basvuru-sil/([^/\.]+)/?$ reddet.php?id=$1
RewriteRule ^basvuru-kabul/([^/\.]+)/?$ kabulet.php?id=$1
RewriteRule ^ebasvurular(/?)$ ebasvurular.php
RewriteRule ^ebasvuru-incele/([^/\.]+)/?$ ebasvuru-incele.php?staff_id=$1
RewriteRule ^karakter/([^/\.]+)/?$ karakter.php?char_name=$1
RewriteRule ^arac/([^/\.]+)/?$ arac1.php?id=$1
RewriteRule ^duyurular(/?)$ duyurular.php
RewriteRule ^duyuru-yayinla(/?)$ duyuru-yayinla.php
RewriteRule ^duyuru-duzenle/([^/\.]+)/?$ duyuru-duzenle.php?id=$1
RewriteRule ^duyuru-sil/([^/\.]+)/?$ duyuru-sil.php?id=$1
RewriteRule ^basvuru(/?)$ basvuru.php
RewriteRule ^oyuncu-cezalandir(/?)$ oyuncu-cezalandir.php